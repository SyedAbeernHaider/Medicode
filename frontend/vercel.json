{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/assets/**",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "statusCode": 200
    }
  ],
  "rewrites": [
    {
      "source": "/adminlogin",
      "destination": "/index.html"
    }
  ]
}
